@model IEnumerable<Product>

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
             <tr>
                <th scope="row">item.Id</th>
                <td>item.Name</td>
                <td>item.Price</td>
            </tr>
        } 
    </tbody>
</table>

